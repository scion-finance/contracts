{
  "name": "@sc1/contracts",
  "version": "0.5.11",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "hardhat test",
    "dev": "yarn export",
    "deploy:local": "hardhat --network localhost deploy",
    "deploy:avalanche": "hardhat --network avalanche deploy",
    "deploy:fantom": "hardhat --network fantom deploy",
    "runScript": "hardhat run",
    "cover": "hardhat coverage --testfiles ",
    "local": "hardhat node",
    "build": "hardhat compile",
    "compile": "hardhat compile",
    "export": "hardhat export --export-all ../common/src/contracts.json",
    "fork:avalanche:fresh": "FORK_CHAIN='avalanche' bash ./scripts/fork_ava.sh",
    "fork:avalanche": "FORK_CHAIN='avalanche' bash ./scripts/fork_ava.sh --no-reset",
    "fork:fantom": "FORK_CHAIN='fantom' bash ./scripts/fork_fantom.sh --no-reset",
    "fork": "INFURA_API_KEY=$(grep INFURA_API_KEY .env | cut -d '=' -f2) bash ./scripts/fork.sh"
  },
  "dependencies": {
    "@anders-t/ethers-ledger": "^1.0.2",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.10",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.3.2",
    "@openzeppelin/contracts-upgradeable": "^4.3.2",
    "@pangolindex/exchange-contracts": "^1.0.2",
    "@typechain/ethers-v5": "^8.0.5",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.0",
    "hardhat": "^2.6.1",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "^0.9.24",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-typechain": "^0.3.5",
    "pub": "^0.2.0",
    "solidity-coverage": "^0.7.16",
    "ts-generator": "^0.1.1",
    "typechain": "^6.0.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0"
  }
}
